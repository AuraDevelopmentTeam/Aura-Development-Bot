<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN" packages="dev.aura.aurabot">
  <Appenders>
    <RandomAccessFile name="TestFile" fileName="logs/test.log">
      <PatternLayout pattern="[%d{yyyy-MM-dd HH:mm:ss.SSS}] [%-20.20c] [%t/%-5level]: %msg%n" />
    </RandomAccessFile>
  </Appenders>
  <Loggers>
    <Root level="trace">
      <AppenderRef ref="TestFile" level="trace"/>
    </Root>
  </Loggers>
</Configuration>
